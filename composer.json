{
    "name": "c0defusi0n/security-scanner",
    "description": "Security scanner to detect malicious code in Magento 2",
    "type": "magento2-module",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "c0defusi0n",
            "homepage": "https://github.com/c0defusi0n"
        }
    ],
    "require": {
        "php": ">=8.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "phpunit/php-code-coverage": "^9.2"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "C0defusi0n\\SecurityScanner\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "C0defusi0n\\SecurityScanner\\Test\\": "Test"
        }
    },

    "minimum-stability": "stable",
    "keywords": [
        "magento",
        "magento2",
        "security",
        "scanner",
        "malware"
    ],
    "support": {
        "issues": "https://github.com/c0defusi0n/security-scanner/issues",
        "source": "https://github.com/c0defusi0n/security-scanner"
    }
}
